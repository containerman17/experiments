{
    "manifest_version": 3,
    "name": "Random Emoji Inserter",
    "version": "1.0",
    "description": "Right-click to insert random emoji at cursor",
    "permissions": [
        "activeTab",
        "scripting",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon.svg",
        "32": "icon.svg",
        "48": "icon.svg",
        "128": "icon.svg"
    }
}
