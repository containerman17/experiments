<?xml version="1.0"?>
<clickhouse>
    <rocksdb>
        <options>
            <max_background_jobs>16</max_background_jobs>
        </options>
        <column_family_options>
            <compression>kZSTD</compression>
            <bottommost_compression>kZSTD</bottommost_compression>
            <write_buffer_size>268435456</write_buffer_size>
            <max_write_buffer_number>6</max_write_buffer_number>
            <min_write_buffer_number_to_merge>2</min_write_buffer_number_to_merge>
            <level0_file_num_compaction_trigger>2</level0_file_num_compaction_trigger>
            <optimize_filters_for_hits>false</optimize_filters_for_hits>
        </column_family_options>
    </rocksdb>
</clickhouse>